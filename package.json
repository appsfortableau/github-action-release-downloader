    "build": "ncc build src/main.ts --license licenses.txt"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mime": "^3.0.0",
    "@types/node": "^18.6.3",
    "@vercel/ncc": "^0.34.0",
    "nodemon": "^2.0.19",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@actions/core": "^1.9.1",
    "@actions/exec": "^1.1.1",
    "@actions/github": "^5.0.3",
    "@octokit/core": "^4.0.4",
    "node-fetch": "2"
  },
  "releases": {
    "super-tables-free": "github.com:appsfortableau/supertables#4.0.0",
    "super-tables-sandbox": "github.com:appsfortableau/supertables#4.0.0"
  }
}
